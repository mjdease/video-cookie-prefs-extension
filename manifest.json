{
  "manifest_version": 2,
  "name": "Video Cookie Preferences",
  "description": "Sets preference cookies on video sites",
  "version": "0.0.1",
  "homepage_url": "https://github.com/mjdease/video-cookie-prefs-extension",
  "permissions": [
    "*://*.youtube.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}